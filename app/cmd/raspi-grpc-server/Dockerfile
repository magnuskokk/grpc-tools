# Extend gobuild which has predownloaded dependencies.
FROM grpc-tools/gobuild as build
WORKDIR /build
RUN go build -a -o bin/raspi-grpc-server cmd/raspi-grpc-server/main.go

# Create a single binary image.
FROM scratch as raspi-grpc-server
COPY --from=build /build/bin/raspi-grpc-server /raspi-grpc-server/raspi-grpc-server
EXPOSE 9001
CMD ["/raspi-grpc-server/raspi-grpc-server"]
