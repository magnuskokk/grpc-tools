# Extend gobuild which has predownloaded dependencies.
FROM grpc-tools/gobuild as build
WORKDIR /build
RUN go build -a -o bin/echo-grpc-server cmd/echo-grpc-server/main.go

# Create a single binary image.
FROM scratch as echo-grpc-server
COPY --from=build /build/bin/echo-grpc-server /echo-grpc-server/echo-grpc-server
EXPOSE 9000
CMD ["/echo-grpc-server/echo-grpc-server"]
