# Extend gobuild which has predownloaded dependencies.
FROM grpc-tools/gobuild as build
WORKDIR /build
RUN go build -a -o bin/echo-gateway-server cmd/echo-gateway-server/main.go

# Create a single binary image.
FROM scratch as echo-gateway-server
COPY --from=build /build/bin/echo-gateway-server /echo-gateway-server/echo-gateway-server
EXPOSE 8000
CMD ["/echo-gateway-server/echo-gateway-server"]
